<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_10_month">

    <RelativeLayout tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_1_month"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Altura y peso en el decimo mes"
            android:id="@+id/textView189"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niño"
            android:id="@+id/textView190"
            android:layout_marginTop="53dp"
            android:layout_below="@+id/textView189"
            android:layout_alignLeft="@+id/textView189"
            android:layout_alignStart="@+id/textView189" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín 8.600 Max. 10.600"
            android:id="@+id/textView191"
            android:layout_below="@+id/textView190"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm) Mín 71.0 Max. 76.3"
            android:id="@+id/textView192"
            android:layout_below="@+id/textView191"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niña"
            android:id="@+id/textView193"
            android:layout_above="@+id/textView194"
            android:layout_alignLeft="@+id/textView190"
            android:layout_alignStart="@+id/textView190" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín. 7.900 Max. 9.900"
            android:id="@+id/textView194"
            android:layout_centerVertical="true"
            android:layout_alignRight="@+id/textView191"
            android:layout_alignEnd="@+id/textView191" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm) Mín. 69.0 Max. 74.5"
            android:id="@+id/textView195"
            android:layout_below="@+id/textView194"
            android:layout_toRightOf="@+id/textView193"
            android:layout_toEndOf="@+id/textView193" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView24"
            android:layout_below="@+id/textView195"
            android:layout_centerHorizontal="true"
            android:src="@drawable/size" />
    </RelativeLayout>
</RelativeLayout>
