<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_9_month">

    <RelativeLayout tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_1_month"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Altura y peso en el noveno mes"
            android:id="@+id/textView169"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niño"
            android:id="@+id/textView170"
            android:layout_marginTop="53dp"
            android:layout_below="@+id/textView169"
            android:layout_alignLeft="@+id/textView169"
            android:layout_alignStart="@+id/textView169" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín 8.200 Max. 10.200"
            android:id="@+id/textView171"
            android:layout_below="@+id/textView170"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm) Mín 69.7 Max. 75.0"
            android:id="@+id/textView172"
            android:layout_below="@+id/textView171"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niña"
            android:id="@+id/textView173"
            android:layout_above="@+id/textView174"
            android:layout_alignLeft="@+id/textView170"
            android:layout_alignStart="@+id/textView170" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín. 7.600 Max. 9.600"
            android:id="@+id/textView174"
            android:layout_centerVertical="true"
            android:layout_alignRight="@+id/textView171"
            android:layout_alignEnd="@+id/textView171" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm)  Mín. 67.7 Max. 73.2"
            android:id="@+id/textView175"
            android:layout_below="@+id/textView174"
            android:layout_toRightOf="@+id/textView173"
            android:layout_toEndOf="@+id/textView173" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView21"
            android:layout_below="@+id/textView175"
            android:layout_centerHorizontal="true"
            android:src="@drawable/size" />
    </RelativeLayout>
</RelativeLayout>
