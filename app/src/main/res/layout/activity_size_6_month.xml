<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_6_month">

    <RelativeLayout tools:context="org.estebanmarroquin.babycare.activities.feeding.size.size_1_month"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Altura y peso en el sexto mes"
            android:id="@+id/textView123"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niño"
            android:id="@+id/textView124"
            android:layout_marginTop="53dp"
            android:layout_below="@+id/textView123"
            android:layout_alignLeft="@+id/textView123"
            android:layout_alignStart="@+id/textView123" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín 6.900 Max. 8.800"
            android:id="@+id/textView125"
            android:layout_below="@+id/textView124"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm) Mín 65.1 Max. 70.5"
            android:id="@+id/textView126"
            android:layout_below="@+id/textView125"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Niña"
            android:id="@+id/textView127"
            android:layout_above="@+id/textView128"
            android:layout_alignLeft="@+id/textView124"
            android:layout_alignStart="@+id/textView124" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="peso(kg) Mín. 6.300 Max. 8.100"
            android:id="@+id/textView128"
            android:layout_centerVertical="true"
            android:layout_alignRight="@+id/textView125"
            android:layout_alignEnd="@+id/textView125" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="talla(cm)  Mín. 63.3 Max. 68.6"
            android:id="@+id/textView129"
            android:layout_below="@+id/textView128"
            android:layout_toRightOf="@+id/textView127"
            android:layout_toEndOf="@+id/textView127" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView12"
            android:layout_below="@+id/textView129"
            android:layout_centerHorizontal="true"
            android:src="@drawable/size" />
    </RelativeLayout>
</RelativeLayout>
