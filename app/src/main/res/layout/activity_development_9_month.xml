<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.estebanmarroquin.babycare.activities.feeding.development.development_9_month">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Desarrollo en el noveno mes"
        android:id="@+id/textView180"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Podría empezar a decir algunas palabras como &quot;Papá o Mamá&quot; indiferenciadamente, aunque otros bebés podrían empezar a hablar después."
        android:id="@+id/textView181"
        android:layout_below="@+id/textView180"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Puede entender muchas palabras simples y de uso común, tales como: &quot;zapato&quot;, &quot;galleta&quot; y &quot;pelota&quot;. Y es muy común que entienda instrucciones simples, como &quot;dame..."
        android:id="@+id/textView182"
        android:layout_below="@+id/textView181"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="56dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cada vez más, el bebé copia lo que hacen los demás. Imita el tono de voz, risas, gestos y expresiones faciales de sus padres, trata de hacer las cosas que ellos hacen."
        android:id="@+id/textView183"
        android:layout_below="@+id/textView182"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="58dp" />
</RelativeLayout>
