<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="org.estebanmarroquin.babycare.activities.feeding.health.health_10_month">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Salud y recomendaciones en su decimo mes"
        android:id="@+id/textView184"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Asegúrate de no tener en casa plantas o arreglos florales tóxicos. Algunas de estas plantas son: el papagayo, orejas de elefante, narcisos y narcisos atrompetados, acebo, muérdago y nochebuena."
        android:id="@+id/textView185"
        android:layout_below="@+id/textView184"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="51dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Consulta a tu pediatra por aquellos alimentos que contengan Prebióticos, DHA, ARA, Colina y Hierro que apoyan el desarrollo mental y físico del bebé."
        android:id="@+id/textView186"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView23"
        android:layout_below="@+id/textView186"
        android:src="@drawable/bebe_papa"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView186"
        android:layout_alignEnd="@+id/textView186"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
